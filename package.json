{
  "name": "ngpress",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run ngpress:serve-ssr",
    "serve:ssr": "node dist/ngpress/server/main.js",
    "build:ssr": "ng build && ng run ngpress:server",
    "prerender": "node prerender-gen.mjs && ng run ngpress:prerender --routes-file routers",
    "analyzer": "ng build --stats-json && webpack-bundle-analyzer dist/ngpress/browser/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/platform-server": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@nguniversal/express-engine": "^14.0.2",
    "express": "^4.15.2",
    "front-matter": "^4.0.2",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.17",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "~14.0.2",
    "@angular/compiler-cli": "^14.0.0",
    "@nguniversal/builders": "^14.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.0.0",
    "@types/marked": "^4.0.3",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.7.2"
  }
}
